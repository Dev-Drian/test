    {
      "pattern": "\\bpa\\b",
      "replacement": "para",
      "flags": "gi",
      "description": "pa → para"
    },
    {
      "pattern": "\\bm([aeiou])s\\b",
      "replacement": "más",
      "flags": "gi",
      "description": "mas → más"
